{
  "ignoreFiles": ["**/*"],
  "overrides": [
    {
      "files": ["**/*.{css,scss}"],
      "extends": [
        "stylelint-config-standard",
        "stylelint-config-standard-scss",
        "stylelint-config-sass-guidelines"
      ],
      "rules": {}
    },
    {
      "files": ["**/*.scss"],
      "extends": ["stylelint-config-standard-scss"],
      "rules": {}
    }
  ],
  "rules": {
    "selector-no-qualifying-type": null
  }
}
